{
    "devDependencies": {
        "@octokit/rest": "^21.1.1",
        "@prettier/plugin-php": "^0.22.4",
        "@prettier/plugin-xml": "^3.4.1",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "@cloudflare/workers-types": "^4.20241112.0",
        "@react-router/dev": "^7.3.0",
        "@types/node": "^20",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.1",
        "dotenv-cli": "^7.4.3",
        "drizzle-kit": "~0.28.1",
        "axios": "^1.7.9",
        "change-case": "^5.4.4",
        "concurrently": "^9.1.2",
        "laravel-vite-plugin": "^1.2.0",
        "postcss": "^8.5.2",
        "prettier": "^3.5.1",
        "prettier-plugin-java": "^2.6.7",
        "prettier-plugin-kotlin": "^2.1.0",
        "prettier-plugin-sh": "^0.14.0",
        "prettier-plugin-toml": "^2.0.1",
        "prompts": "^2.4.2",
        "tailwindcss": "^3.4.17",
        "vite": "^6.1.0",
        "wrangler": "^3.87.0",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "private": true,
    "scripts": {
        "see": "git status --short | grep '^ M'",
        "typecheck": "react-router typegen && tsc -b",
        "build": "react-router build",
        "db:generate": "dotenv -- drizzle-kit generate",
        "db:migrate": "wrangler d1 migrations apply --local DB",
        "db:migrate-production": "dotenv -- drizzle-kit migrate",
        "deploy": "npm run build && wrangler deploy",
        "start": "wrangler dev",
        "test:all": "pytest;  bun run test:dart;  bun test test;  bun run test:kt;  bun run test:php;  go test  ./{easy,medium,very_easy,very_hard,hard}",
        "lint": "bunx --bun prettier --write {src,test}",
        "test:dart": "dart test -r expanded --concurrency=20",
        "test:bun": "bunx  test ",
        "test:kt": "gradle test --parallel",
        "test:php": "./vendor/bin/pest --parallel",
        "lint:php": "./vendor/bin/duster fix",
        "lint:dart": "dart format ./;  dart fix --apply",
        "lint:swift": "swift-format format -r -i .",
        "lint:black": "black ./;  isort ./",
        "lint:go": "go fmt  ./{easy,medium,very_easy,very_hard,hard}",
        "lint:yapf": "yapf  -i -r ./;  isort ./",
        "test:swift": "swift test  --enable-code-coverage --parallel --enable-swift-testing",
        "lint:all": "bun run lint;  bun run lint:dart;  bun run lint:black;  bun run lint:swift;  bun run lint:kt;  bun run lint:php;  bun run lint:go",
        "docs:go": "godoc -http=:6060",
        "lint:kt": "gradle spotlessApply --continue",
        "dev": "react-router dev"
    },
    "type": "module",
    "dependencies": {
        "@paralleldrive/cuid2": "^2.2.2",
        "@pinia/testing": "^1.0.1",
        "@react-router/node": "^7.3.0",
        "@react-router/serve": "^7.3.0",
        "@tanstack/react-router": "^1.112.17",
        "@tanstack/react-start": "^1.112.17",
        "drizzle-orm": "~0.36.3",
        "isbot": "^5.1.17",
        "pinia": "^3.0.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router": "^7.3.0",
        "vinxi": "^0.5.3",
        "vue": "^3.5.13"
    },
    "packageManager": "yarn@3.5.1+sha512.8cd0e31bd60779ef4ca92b855fb3462c7ec35ce8b345752b7349a68239776417f46d41d79c8047242d9c93b48a1516f64c7444ebe747d9a02bf26868e6fa1f2b"
}
